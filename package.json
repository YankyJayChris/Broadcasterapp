{
  "name": "server",
  "version": "1.0.0",
  "description": "Broadcaster server",
  "main": "server.js",
  "scripts": {
    "test": "nyc mocha --require @babel/polyfill --require @babel/register server/tests/**/*.js* --recursive --exit",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "local:coveralls": "nyc npm run test && nyc report --reporter=text-lcov | coveralls",
    "serve": "nodemon --exec babel-node server/server.js",
    "start": "babel-node -- server/server.js"
  },
  "dependencies": {
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "@hapi/joi": "^16.1.7",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "multer": "^1.4.2",
    "pg": "^7.14.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.2.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YankyJayChris/Broadcasterapp.git"
  },
